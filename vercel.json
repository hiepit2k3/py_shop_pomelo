{
  "version": 2,
  "builds": [
    {
      "src": "web/run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web/app/api/routers_user.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/app/api/routers_product.py"
    }
  ]
}
